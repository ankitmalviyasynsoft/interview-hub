{
  "name": "interview-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm rebuild lightningcss || true",
    "dev": "next dev -p 3001",
    "build": "next build",
    "lint": "eslint",
    "postbuild": "npm run copy-standalone-assets",
    "copy-standalone-assets": "shx mkdir -p .next/standalone/.next/static && shx cp -r .next/static/* .next/standalone/.next/static && shx mkdir -p .next/standalone/public && shx cp -r public/* .next/standalone/public",
    "start:without-standalone": "next start",
    "start": "node .next/standalone/server.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@reduxjs/toolkit": "^2.11.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^10.1.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "mongoose": "^9.1.5",
    "next": "16.1.6",
    "next-swagger-doc": "^0.4.1",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-quill-new": "^3.8.2",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "shx": "^0.4.0"
  }
}
